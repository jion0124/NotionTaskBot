{
  "name": "notiontaskbot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "security-check": "tsx scripts/security-check.ts",
    "bot": "tsx src/bot/index.ts",
    "bot:dev": "nodemon --exec tsx src/bot/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run bot:dev\"",
    "start:all": "concurrently \"npm run start\" \"npm run bot\"",
    "dev:web": "npm run dev",
    "dev:bot": "npm run bot:dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@notionhq/client": "^3.1.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.50.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.523.0",
    "next": "^14.2.5",
    "openai": "^5.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint-config-next": "^14.2.5",
    "nodemon": "^3.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2"
  }
}
